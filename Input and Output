#Hello World

print("Hello World")#python

#Hello World with tab

print("Hello World\tHello World")#python

#Hello World with new line

print('Hello World \n Hello World')

#Integer Input

a=int(input())
print(a)#python

#Float Input

a=float(input())
print('%2f'%a)#python

#Character Input

a=input()
print(a)#python

#String Input

a=str(input())
print(a)#python

#Ascii-I

a=input()
print(ord(a))#python

#Ascii-II

a=int(input())
print(chr(a))#python
